{
  "version": 1,
  "settings": {
    "displayLang": "zh-cn",
    "githubProxy": "github",
    "showNotSupportedFile": false,
    "showSearchResultFullPath": false,
    "showRibbon": true,
    "showFileTabs": true,
    "openLinkInCurrentWin": true,
    "quickOpenInCurrentWin": true,
    "ignoreFile": true,
    "ignoreFileGlob": ".git",
    "ribbonState": {
      "core.search": {
        "visible": true,
        "order": 0
      },
      "core.file-explorer": {
        "visible": true,
        "order": 1
      },
      "core.outline": {
        "visible": true,
        "order": 2
      }
    }
  }
}